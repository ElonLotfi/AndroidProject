<?xml version="1.0" encoding="utf-8"?>


<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.plfgb.app" >
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:usesCleartextTraffic="true"
        android:name=".GlobalState"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >

        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DrawingActivity" />
        <activity android:name=".DrawingSquare" />
        <activity android:name=".DrawingCircle" />
        <activity android:name=".DrawingTriangle" />
        <activity android:name=".Game.GameActivity" />
        <activity android:name=".Game.GameLauncherActivity" />
        <activity android:name=".MenuActivity" />




    </application>

</manifest>
